{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start -p 3400 --no-open",
    "start:zh": "docusaurus start -p 3400 --locale zh-Hans --no-open",
    "build:docs": "docusaurus build",
    "build:speech": "cd ./speech/intro && yarn run build && node ../../scripts/moveSpeech.js",
    "build": "yarn run build:speech && yarn run build:docs",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-translations:zh": "docusaurus write-translations --locale zh-Hans",
    "write-heading-ids": "docusaurus write-heading-ids",
    "postinstall": "cd ./speech/intro && yarn"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.0",
    "@docusaurus/preset-classic": "2.0.0-beta.0",
    "@docusaurus/remark-plugin-npm2yarn": "^2.0.0-beta.0",
    "@mdx-js/react": "^1.6.21",
    "@svgr/webpack": "^5.5.0",
    "clsx": "^1.1.1",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "url-loader": "^4.1.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.0.0-beta.0",
    "@tsconfig/docusaurus": "^1.0.2",
    "@types/react": "^17.0.9",
    "@types/react-helmet": "^6.1.1",
    "@types/react-router-dom": "^5.1.7",
    "typescript": "^4.3.2"
  }
}
